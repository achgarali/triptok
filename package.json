{
  "name": "triptok",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run prisma:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest --run",
    "test:watch": "vitest",
    "db:generate": "PRISMA_GENERATE_DATAPROXY=false prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "prisma:generate": "rm -rf node_modules/.prisma && PRISMA_GENERATE_DATAPROXY=false PRISMA_ENGINE_CHECK_BINARY_EXISTS=false prisma generate",
    "postinstall": "rm -f .env.production && npm run prisma:generate",
    "build:prod": "npm run prisma:generate && prisma migrate deploy && next build"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "bcrypt": "^5.1.1",
    "leaflet": "^1.9.4",
    "nanoid": "^5.1.6",
    "next": "^14.2.0",
    "next-auth": "^4.24.0",
    "postcss": "^8.4.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-leaflet": "^4.2.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "@types/bcrypt": "^5.0.2",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.0",
    "fast-check": "^3.22.0",
    "prisma": "^5.20.0",
    "vitest": "^2.0.0"
  }
}
