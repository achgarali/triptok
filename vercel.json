{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["cdg1"],
  "env": {
    "NODE_ENV": "production",
    "PRISMA_GENERATE_DATAPROXY": "false"
  },
  "build": {
    "env": {
      "PRISMA_GENERATE_DATAPROXY": "false",
      "SKIP_ENV_VALIDATION": "true"
    }
  },
  "installCommand": "unset PRISMA_DATABASE_URL && npm install",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}

