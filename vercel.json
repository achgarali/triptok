{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "PRISMA_GENERATE_DATAPROXY=false npm install",
  "framework": "nextjs",
  "regions": ["cdg1"],
  "env": {
    "NODE_ENV": "production",
    "PRISMA_GENERATE_DATAPROXY": "false",
    "PRISMA_ENGINE_CHECK_BINARY_EXISTS": "false"
  },
  "build": {
    "env": {
      "PRISMA_GENERATE_DATAPROXY": "false",
      "PRISMA_ENGINE_CHECK_BINARY_EXISTS": "false",
      "PRISMA_CLI_BINARY_TARGETS": "linux-musl-openssl-3.0.x,linux-musl-arm64-openssl-3.0.x,rhel-openssl-1.0.x",
      "SKIP_ENV_VALIDATION": "true"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}

